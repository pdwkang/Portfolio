}

.fly{
	position: absolute;
	border: 2px solid white;
	font-family: sans-serif;
	color: white;
	width: 33px;
	height: 33px;
	font-size: 19px;
	border-radius: 50%;
	text-align: center;
	padding:1px;
}
.fly-number{
	font-weight: 800;
}
.letter1{left:20px; animation: bounceLetters .44s infinite linear;}
.letter2{left:60px; animation: bounceLetters .32s infinite linear;}
.letter3{left:20px; animation: bounceLetters2 .42s infinite linear;}
.letter4{left:80px; animation: bounceLetters2 .31s infinite linear;}
.letter5{left:120px; animation: bounceLetters2 .43s infinite linear;}
// .letter6{top:65px; left:170px; animation: bounceLetters .35s infinite linear;}
// .letter7{top:65px; left:200px; animation: bounceLetters .35s infinite linear;}
// .letter8{top:65px; left:230px; animation: bounceLetters .35s infinite linear;}
// .letter9{top:65px; left:260px; animation: bounceLetters .35s infinite linear;}

@keyframes bounceLetters{
	0%{
		top:20px
	}
	50%{
		top:24px
	}
	100%{
		top:20px
	}
}
@keyframes bounceLetters2{
	0%{
		top:70px
	}
	50%{
		top:74px
	}
	100%{
		top:70px
	}
}
.letterK{
	animation: lk 4s infinite linear;

}
@keyframes lk{
	0%{top: 120px;left: 20px;}
	30%{top: 220px; transform:rotate(10deg)}
	35%{top: 120px;left: 230px;transform: rotate(-2000deg);}
	40%{top: 120px;left: 20px;transform: rotate(0deg)}
	100%{top: 120px;left: 20px;}
}
.letterA{
	animation: la2 4s infinite linear;
}
@keyframes la2{
	0%{top: 120px;left: 60px;}
	25%{top: 220px; transform:rotate(10deg)}
	30%{top: 120px;left: 230px;transform: rotate(-2000deg);}
	35%{top: 120px;left: 60px;transform: rotate(0deg);}
	100%{top: 120px;left: 60px;}
}
.letterN{
	animation: ln 4s infinite linear;
}
@keyframes ln{
	0%{top: 120px;left: 100px;}
	20%{top: 220px; transform:rotate(10deg)}
	25%{top: 120px;left: 230px;transform: rotate(-2000deg);}
	30%{top: 120px;left: 100px;transform: rotate(0deg);}
	100%{top: 120px;left: 100px;}
}
.letterG{
	animation: lg 4s infinite linear;
	
}

@keyframes lg{
	0%{top: 120px;left: 140px;}
	15%{top: 220px; transform:rotate(10deg)}
	20%{top: 120px;left: 230px;transform: rotate(-2000deg);}
	25%{top: 120px;left: 140px;transform: rotate(0deg)}
	100%{top: 120px;left: 140px;}
}

