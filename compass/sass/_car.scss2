.rim{
	height: 54px;
	width:  54px;
	border-radius: 50%;
	background-color: white;
	position: relative;
	margin: 10px;
	border: 2px solid black;
}

.rotate {
    animation: spin .3s infinite linear;
}

.moveUp {
    animation: moveUp .4s infinite linear;
}

@keyframes spin {
    100% {
        transform:rotate(-360deg);
    }
}
@keyframes blink {
	0% {
		// border-bottom-color: rgba(100,150,200,.8);
		border-bottom-color: rgba(0,0,0,.1);
	}
	50%{
		// border-bottom-color: rgba(250,250,250,0.8);
		border-bottom-color: rgba(0,0,0,.4);
	} 
	100%{
		// border-bottom-color: rgba(100,150,200,.8);
		border-bottom-color: rgba(0,0,0,.1);
	}
}
 
@keyframes moveUp {
    0% {
	margin-top: 10px;
    }
    50% {
	margin-top: 15px;
    }
    100% {
	margin-top: 10px;
    }
}

.rim-inside{
	position: absolute;
	border-bottom: 2px solid black;
}
.r1{
	height: 24px;
	width: 50px;
}
.r2{
	transform: rotate(45deg);
	height: 29px;
	width: 50px;	
	margin-top: 2px;
	margin-left: 9px;
}
.r3{
	transform: rotate(-45deg);
	height: 1px;
	width: 52px;	
	margin-top: 23px;
}

.rim-inside-circle{
	background-color: white;
	position: absolute;
	margin-top: 17px;
	margin-left: 17px;
	height: 15px;
	width: 15px;
	border-radius: 50%;
	border: 2px solid black;
}

.road{
	padding-top: 5px;
	height: 50px;
	width: 100vw;
	// background: linear-gradient(to top, rgba(240,232,240,1) 0%, rgba(240,232,240,1) 100%);		
	animation: evenOutMargin .4s infinite linear;
}

@keyframes evenOutMargin{
	0% {
	margin-top: 15px;
    }
    50% {
	margin-top: 10px;
    }
    100% {
	margin-top: 15px;
    }
}

.white-line{
	height: 12px;
	width: 100px;
	background-color: white;
	animation: pass .7s infinite linear;
}

@keyframes pass{
	0%{margin-left:0%;}
	20%{margin-left:20%;}
	40%{margin-left:40%;}
	60%{margin-left:60%;}
	80%{margin-left:80%;}
	100%{margin-left:100%;}
}

